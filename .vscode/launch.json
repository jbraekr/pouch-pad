    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "internal db",
            "program": "${workspaceFolder}/server.js"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "external db",
            "program": "${workspaceFolder}/server.js",
            "env": {
                "DB": "http://192.168.43.193:3000/db/kittens"
            }
        }
    ],
    "compounds": [
        {
            "name": "Compound",
            "configurations": [
                "internal db",
                "external db"
            ]
        }
    ]
}